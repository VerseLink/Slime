{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", ".output/**", ".wxt/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "passThroughEnv": ["PATH", "LOCALAPPDATA", "PROGRAMFILES", "PROGRAMFILES(X86)", "CHROME_PATH", "LIGHTHOUSE_CHROMIUM_PATH"]
    }
  }
}
